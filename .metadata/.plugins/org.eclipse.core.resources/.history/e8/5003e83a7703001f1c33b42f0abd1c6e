package com.kh.variable.pre;

public class EnumPre {
// enum 무지개{빨~보}
	public enum 무지개{
		빨,
		주,
		노,
		초,
		파,
		남,
		보,
	}
	//main
			// 빨~보 출력
	public static void main(String[] args) {
		System.out.println("빨 : "+무지개.빨);
		System.out.println("주 : "+무지개.주);
		System.out.println("노 : "+무지개.노);
		System.out.println("초 : "+무지개.초);
		System.out.println("파 : "+무지개.파);
		System.out.println("남 : "+무지개.남);
		System.out.println("보 : "+무지개.보);
	
	}
	
	
}
