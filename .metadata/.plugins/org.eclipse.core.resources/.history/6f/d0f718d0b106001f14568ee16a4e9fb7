package com.kh.test.main;

import java.util.Scanner;

public class Test4 {
	
	public static void main(String[] args) {
	/*
	 1부터 9까지 값을 스캐너로 각각 입력 받아
	 
	  더하기 빼기 곱하기 몱 출력하기
	  
	  int 숫자1
	  int 숫자2
	  */	
		Scanner 입력 = new Scanner(System.in);
		
		int 숫자1 = 입력.nextInt();
		System.out.println("숫자1를 입력하세요" + 입력);
		int 숫자2 = 입력.nextInt();
		System.out.println("숫자2를 입력하세요" + 입력);
		
		
		}
	
	}
}
