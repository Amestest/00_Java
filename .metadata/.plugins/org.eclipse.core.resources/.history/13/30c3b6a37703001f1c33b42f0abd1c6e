package com.kh.variable.pre;

public class EnumPre {
// enum 무지개{빨~보}
	public enum 무지개{
		빨,주,노,초,파,남,보,
	}
	//main
			// 빨~보 출력
	public static void main(String[] args) {
		
		//장바구니 = 배열에 담기
		무지개[] 배열 = 무지개.values();//enum으로 작성한 값을 가지고옴 
		//for - each
		for(무지개 무 : 배열) {
			System.out.println(무);
		}
	}
	
	
}
